			pinctrl-0 = <&int_active &tkey_led_active>;
			pinctrl-1 = <&int_suspend &tkey_led_suspend>;

			vdd_ldo_name = "VDD_TSP_3.0V";
//			vdd_en-gpio = <&msm_gpio 73 0x01>;
                        keyled_en-gpio = <&msm_gpio 56 0x00>;
